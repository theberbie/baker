<div class="booyah-box col-xs-10 col-xs-offset-1">
<h1> Add your goods! </h1>
<%= simple_form_for @product, url: artisan_products_path do |f| %>
  <%= f.input :name %>
  <%= f.input :description %>
  <%= f.input :category %>
  <%= f.input :cost %>
  <br />
  <%= f.submit 'Add!', class:'btn btn-success' %>
  <br />
  <% end %>
  <br />
</div>